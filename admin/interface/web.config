<?xml version="1.0"?>
<configuration>
	<system.webServer>
		<security>
		    <ipSecurity allowUnlisted="false">    <!-- this line blocks everybody, except those listed below -->                
		        <clear/>
		        <add ipAddress="127.0.0.1" allowed="true"/>
		    </ipSecurity>
		</security>
	</system.webServer>
</configuration>